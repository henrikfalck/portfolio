{
  "name": "henrikfalck",
  "private": false,
  "description": "A simple start with a simple framework, plus a few custom commands and snippets.",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "update": "clear && echo \"Checking for updated packages...\" && echo \"\" && sleep 4 && yarn upgrade -L && sleep 2 && clear && echo \"Packages updated.\""
  },
  "dependencies": {
    "gatsby": "^2.23.4",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-google-analytics": "^2.3.4",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-scroll-reveal": "^0.0.7",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-source-contentful": "^2.3.12",
    "gatsby-source-graphql": "^2.5.5",
    "gatsby-source-wordpress": "^3.3.15",
    "gatsby-transformer-sharp": "^2.5.5",
    "netlify-plugin-a11y": "0.0.12",
    "netlify-plugin-gatsby-cache": "^0.2.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-loader": "^4.0.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henrikfalck/portfolio"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
